<h1>List of machines</h1>
<li><%= link_to 'Sign out', destroy_user_session_path, method: :delete %></li>
<br />
    <%@machines.each do |m|%>
    <div class = "booyah-box col-xs-10 col-xs-offset-1">
      <h2>Name:<%=m.name%></h2>
      <p>Description: <%=m.description%></p>
      <p>Location: <%=m.location%></p>
      <p>Installation Year:<%=m.year%></p>
      <div class="row"><%= link_to 'Edit', edit_machine_path(m), class: 'btn btn-primary btn-lg'%>
      <%= link_to 'View details', machine_path(m), class: 'btn btn-primary btn-lg'%></div>
    </div>
    <%end%>
    
    


  
      

