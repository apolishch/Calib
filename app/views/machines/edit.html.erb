<div class="booyah-box col-xs-6 col-xs-offset-1">
  <h1>Input Your Machine Details</h1>
  <%=simple_form_for @machine do |f| %>
    <%=f.input :name%>
    <%=f.input :description%>
    <%=f.input :location%>
    <%=f.input :year%>
    <br />
    <%=f.submit 'Edit', class: 'btn btn-primary'%>
  <%end%>
  <br />
</div>